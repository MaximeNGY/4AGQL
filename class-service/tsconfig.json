{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".", // Ajout de baseUrl pour les chemins relatifs
    "paths": {
      "user-service/*": ["../user-service/*"] // Remplace "user-service" par le chemin appropri√©
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
